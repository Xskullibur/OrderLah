{
  "name": "OrderLah",
  "version": "1.0.0",
  "description": "An ordering system which enable seamless transactions between the customer and server by minimizing interactions.",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha --timeout 5000",
    "compile-sass": "sass -I node_modules --no-source-map ./sass:./public/css",
    "start": "npm run compile-sass && node index.js",
    "start:nocompilesass": "node index.js",
    "start:dev": "nodemon --watch sass --watch server --watch ./index.js --watch ./dummy.js -e js,sass,css"
  },
  "keywords": [
