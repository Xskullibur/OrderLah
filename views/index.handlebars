<link rel="stylesheet" href="css/menu_item.css"/>
<div style="min-height: 1000px; width: 100vw;">
    <div class="container-fluid p-5">
        <div class="row">
            <div class="col-3">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="float-right">
                                <ul class="menu-list p-5 list-group list-group-flush text-center" >
                                    <li class="list-group-item">Item 1</li>
                                    <li class="list-group-item">Item 2</li>
                                    <li class="list-group-item">Item 3</li>
                                    <li class="list-group-item">Item 4</li>
                                    <li class="list-group-item">Item 5</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col"> 
                {{#each size}}
                    {{> menu_item index=@index}}
                {{/each}}
            </div>
        </div>
    </div>
</div>
<script src="js/menu_item.js" type="text/javascript"></script>